{
	"compilerOptions": {
		"target": "es2022",
		"lib": ["es2022"],
		"module": "es2022",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"allowJs": true,
		"checkJs": false,
		"noEmit": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"types": ["node", "./worker-configuration.d.ts"]
	},
	"exclude": [".wrangler/**", "dist/**", "node_modules/**"],
	"include": ["worker-configuration.d.ts", "src/**/*.ts", "configs/**/*.ts"]
}
